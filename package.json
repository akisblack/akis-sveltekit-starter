{
	"name": "akis-sveltekit-starter",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@iconify-json/fluent": "^1.1.5",
		"@iconify-json/ic": "^1.1.2",
		"@iconify-json/simple-icons": "^1.1.9",
		"@playwright/test": "^1.21.1",
		"@sveltejs/adapter-static": "next",
		"@sveltejs/kit": "next",
		"@types/html-minifier": "^4.0.2",
		"@typescript-eslint/eslint-plugin": "^5.20.0",
		"@typescript-eslint/parser": "^5.20.0",
		"@unocss/reset": "^0.42.0",
		"autoprefixer": "^10.4.4",
		"cssnano": "^5.1.7",
		"eslint": "^8.14.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^3.4.1",
		"html-minifier": "^4.0.0",
		"mdsvex": "^0.10.5",
		"postcss": "^8.4.12",
		"postcss-extend": "^1.0.5",
		"postcss-grid-kiss": "^3.0.3",
		"postcss-load-config": "^3.1.4",
		"postcss-nested": "^5.0.6",
		"postcss-scss": "^4.0.3",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"prettier-plugin-tailwindcss": "^0.1.10",
		"svelte": "^3.47.0",
		"svelte-check": "^2.7.0",
		"svelte-dark-mode": "^2.1.0",
		"svelte-preprocess": "^4.10.6",
		"svelte-seo": "^1.4.0",
		"svelte-tooltip": "^1.2.0",
		"tslib": "^2.4.0",
		"typescript": "~4.7.4",
		"unocss": "^0.31.13",
		"unocss-preset-better-nested-colors": "^0.1.1"
	},
	"type": "module"
}
