		"postcss-simple-vars": "^6.0.3",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"prettier-plugin-tailwindcss": "^0.1.8",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.10.1",
		"svelte-seo": "^1.4.0",
		"svelte-tooltip": "^1.2.0",
